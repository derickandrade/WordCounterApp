<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="640.0" prefWidth="1015.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tp2.app.TP2Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Pane prefHeight="600.0" prefWidth="900.0">
      <children>
         <Button fx:id="loadFileButton" layoutX="16.0" layoutY="392.0" mnemonicParsing="false" onAction="#onLoadFileButtonClick" text="Carregar" />
         <Label fx:id="fileText" layoutX="100.0" layoutY="396.0" prefHeight="16.0" prefWidth="345.0" text="Nenhum arquivo selecionado" />
         <TextArea fx:id="inputTextArea" layoutX="14.0" layoutY="14.0" prefHeight="368.0" prefWidth="431.0" />
         <TextField fx:id="filePathField" layoutX="16.0" layoutY="423.0" prefHeight="24.0" prefWidth="431.0" />
         <TextArea fx:id="resultField" layoutX="455.0" layoutY="14.0" prefHeight="368.0" prefWidth="431.0" />
         <Button fx:id="startButton" layoutX="828.0" layoutY="423.0" mnemonicParsing="false" onAction="#setStartButton" text="Iniciar" />
      </children>
   </Pane>
</VBox>
